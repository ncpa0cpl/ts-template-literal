{
  "name": "ts-template-literal",
  "displayName": "ts-template-literal",
  "description": "Syntax highlighting for javascript and typescript in template literals.",
  "version": "1.0.0",
  "publisher": "ncpa0cpl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ncpa0cpl/ts-template-literal"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "inline.lit-js",
        "path": "./syntaxes/lit-js.json",
        "embeddedLanguages": {
          "meta.embedded.block.js": "js",
          "meta.embedded.block.ts": "ts"
        }
      }
    ]
  }
}
